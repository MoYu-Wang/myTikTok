{
  "name": "my_tiktok",
  "version": "1.0.0",
  "author": "MoYuWang",
  "description": "tiktok",
  "devDependencies": {
    "electron": "^29.1.4",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package": "electron-packager . 抖音(低仿版) --platform=win32 --arch=x64 --out=dist --overwrite"
  },
  "build": {
    "productName": "mytiktok",
    "appId": "com.example.mytiktok",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis"
    },
    "electronDist": "node_modules/electron/Cache"
  }
}
